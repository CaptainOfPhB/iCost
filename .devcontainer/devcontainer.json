{
	"name": "iCost",
	"build": {
		"context": "..",
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--privileged"
	],
	"customizations": {
		"vscode": {
			"extensions": [
				"dbaeumer.vscode-eslint",
				"octref.vetur",
				"GitHub.copilot",
				"eamodio.gitlens",
				"steoates.autoimport",
				"bungcip.better-toml",
				"streetsidesoftware.code-spell-checker",
				"ms-vscode.vscode-typescript-next",
				"esbenp.prettier-vscode",
				"rvest.vs-code-prettier-eslint",
				"ZixuanChen.vitest-explorer",
				"Vue.volar",
				"adpyke.codesnap"
			]
		}
	},
	"forwardPorts": [
		8080
	],
	"remoteUser": "root"
}